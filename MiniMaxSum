
#include <iostream>
using namespace std;

int main() {
    long min=1000000,max=0,i,val, totalsum=0,Minsum,Maxsum;
    int  arr[100]={8,5,3,9,2};

    for(i=0;i<5;i++){
        totalsum = totalsum+arr[i];
        if(arr[i]< min){
            min=arr[i];
        }
        if(arr[i]>max){
            max=arr[i];
        }
    }
    Maxsum=totalsum-min;
    Minsum=totalsum-max;
    
    cout<<"min sum ="<<minsum<<" & "<<"max sum ="<<maxsum<<endl;
    
}
